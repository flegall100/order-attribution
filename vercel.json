{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/webhook.py": {
      "runtime": "python3.9"
    },
    "api/status.js": {
      "runtime": "nodejs18.x"
    }
  }
} 
